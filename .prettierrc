{
  "singleQuote": true,
  "jsxSingleQuote": true,
  "semi": false,
  "tabWidth": 2,
  "bracketSpacing": true,
  "jsxBracketSameLine": false,
  "arrowParens": "always",
  "trailingComma": "none",
  "printWidth": 120,
  "quoteProps": "as-needed",
  "useTabs": false,
  "overrides": [
    {
      "files": ["*.json", "*.md", ".prettierrc", "*.yml"],
      "options": {
        "useTabs": false
      }
    }
  ]
}
